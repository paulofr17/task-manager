generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Task {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  description String
  priority    Priority
  duration    String
}

enum Priority {
  High
  Medium
  Low
}
